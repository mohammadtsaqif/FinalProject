<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Destination" android:background="@color/default_background" android:id="@+id/m">

    <ImageView
            android:layout_width="393dp"
            android:layout_height="106dp" app:srcCompat="@drawable/background_shape"
            android:id="@+id/background_shappe" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.111" android:layout_marginTop="16dp"/>
    <ImageView
            android:layout_width="121dp"
            android:layout_height="107dp" app:srcCompat="@drawable/cars" android:id="@+id/carss"
            app:layout_constraintTop_toTopOf="@+id/background_shappe"
            app:layout_constraintBottom_toBottomOf="@+id/background_shappe"
            app:layout_constraintStart_toStartOf="@+id/background_shappe"
            app:layout_constraintEnd_toEndOf="@+id/background_shappe" app:layout_constraintHorizontal_bias="0.051"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="217dp"
            android:layout_height="29dp" app:srcCompat="@drawable/ehailing_words" android:id="@+id/ehailing_wordss"
            app:layout_constraintTop_toTopOf="@+id/background_shappe" app:layout_constraintStart_toStartOf="@+id/carss"
            app:layout_constraintBottom_toBottomOf="@+id/background_shappe"
            app:layout_constraintEnd_toEndOf="@+id/background_shappe"
            app:layout_constraintHorizontal_bias="0.746" app:layout_constraintVertical_bias="0.545"/>
    <ImageView
            android:layout_width="120dp"
            android:layout_height="75dp" app:srcCompat="@drawable/chancellar_hall"
            android:id="@+id/chancellar_hall"
            app:layout_constraintTop_toBottomOf="@+id/background_shappe" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" android:layout_marginStart="48dp"/>
    <ImageView
            android:layout_width="130dp"
            android:layout_height="82dp" app:srcCompat="@drawable/oval_park" android:id="@+id/imageView5"
            app:layout_constraintStart_toEndOf="@+id/chancellar_hall"
            app:layout_constraintTop_toBottomOf="@+id/background_shappe" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="9dp" app:layout_constraintHorizontal_bias="0.513"/>
    <Button
            android:text="CHANCELLAR HALL"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_ch"
            app:layout_constraintTop_toBottomOf="@+id/chancellar_hall"
            app:layout_constraintEnd_toEndOf="@+id/chancellar_hall"
            app:layout_constraintStart_toStartOf="@+id/chancellar_hall" android:textColor="#000000"
            android:textSize="10sp" app:layout_constraintHorizontal_bias="0.481"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"/>
    <Button
            android:text="AN-NUR MOSQUE"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_mosque"
            android:textColor="#000000"
            android:textSize="10sp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/mosque"
            app:layout_constraintStart_toStartOf="@+id/mosque" app:layout_constraintEnd_toEndOf="@+id/mosque"/>
    <Button
            android:text="RESIDENTAL VILLAGE"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_village"
            android:textColor="#000000"
            android:textSize="10dp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/imageView7"
            android:layout_marginTop="2dp" android:paddingLeft="6dp" android:paddingRight="6dp"
            app:layout_constraintStart_toStartOf="@+id/imageView7" app:layout_constraintEnd_toEndOf="@+id/imageView7"/>
    <Button
            android:text="LABORATORIES"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_laboratoies"
            android:textColor="#000000"
            android:textSize="10sp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/lab" app:layout_constraintEnd_toEndOf="@+id/lab"
            app:layout_constraintStart_toStartOf="@+id/lab"/>
    <Button
            android:text="CHANCELLAR HALL"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_sport_complex"
            android:textColor="#000000"
            android:textSize="10sp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/sport_complex"
            app:layout_constraintStart_toStartOf="@+id/sport_complex"
            app:layout_constraintEnd_toEndOf="@+id/sport_complex"/>
    <ImageView
            android:layout_width="159dp"
            android:layout_height="72dp" app:srcCompat="@drawable/irc" android:id="@+id/imageView10"
            app:layout_constraintStart_toStartOf="@+id/btn_laboratoies"
            app:layout_constraintEnd_toEndOf="@+id/btn_sport_complex" app:layout_constraintHorizontal_bias="0.556"
            app:layout_constraintTop_toBottomOf="@+id/btn_sport_complex" android:layout_marginTop="12dp"/>
    <Button
            android:text="INFRORMATION RESOURCE CENTER"
            android:layout_width="208dp"
            android:layout_height="38dp" android:id="@+id/btn_irc"
            android:textColor="#000000"
            android:textSize="10dp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/imageView10"
            app:layout_constraintStart_toStartOf="@+id/imageView10"
            app:layout_constraintEnd_toEndOf="@+id/imageView10" android:paddingLeft="2dp" android:paddingRight="2dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="OVAL PARK"
            android:layout_width="140dp"
            android:layout_height="34dp" android:id="@+id/btn_oval_park"
            android:textColor="#000000"
            android:textSize="10sp"
            android:backgroundTint="@color/UTP_yellow" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" app:layout_constraintEnd_toEndOf="@+id/imageView5"
            app:layout_constraintStart_toStartOf="@+id/imageView5"/>
    <ImageView
            android:layout_width="143dp"
            android:layout_height="79dp" app:srcCompat="@drawable/annur_mosque" android:id="@+id/mosque"
            app:layout_constraintTop_toBottomOf="@+id/btn_ch"
            app:layout_constraintStart_toStartOf="@+id/chancellar_hall"
            app:layout_constraintEnd_toEndOf="@+id/chancellar_hall" android:layout_marginTop="24dp"
            app:layout_constraintHorizontal_bias="0.588"/>
    <ImageView
            android:layout_width="138dp"
            android:layout_height="101dp" app:srcCompat="@drawable/sport_complex" android:id="@+id/sport_complex"
            app:layout_constraintTop_toBottomOf="@+id/btn_village"
            android:layout_marginTop="4dp" app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="@+id/imageView7" app:layout_constraintHorizontal_bias="0.291"/>
    <ImageView
            android:layout_width="121dp"
            android:layout_height="100dp" app:srcCompat="@drawable/laboratories" android:id="@+id/lab"
            app:layout_constraintTop_toBottomOf="@+id/btn_mosque"
            android:layout_marginTop="4dp" app:layout_constraintEnd_toEndOf="@+id/mosque"
            app:layout_constraintStart_toStartOf="@+id/mosque"/>
    <ImageView
            android:layout_width="169dp"
            android:layout_height="78dp" app:srcCompat="@drawable/residental_village"
            android:id="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/btn_oval_park"
            app:layout_constraintStart_toStartOf="@+id/imageView5" app:layout_constraintEnd_toEndOf="@+id/imageView5"
            android:layout_marginTop="24dp" app:layout_constraintHorizontal_bias="0.486"/>
</androidx.constraintlayout.widget.ConstraintLayout>