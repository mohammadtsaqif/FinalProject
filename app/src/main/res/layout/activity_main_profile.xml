<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainProfile" android:background="#EFF0FC">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="410dp"
            android:layout_height="304dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            tools:paddingBottom="8dp" android:id="@+id/constraintLayout2">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/top_background"
                android:id="@+id/imageView" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="46dp"
                android:scaleType="fitXY" app:layout_constraintHorizontal_bias="0.0"
        />
        <TextView
                android:text="HELLO,"
                android:textSize="25sp"
                android:textStyle="bold"
                android:textColor="@color/black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/Username"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="76dp" android:layout_marginStart="28dp"
        />
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/name"
                android:textSize="25sp"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/Username"
                android:layout_marginTop="32dp"
                app:layout_constraintHorizontal_bias="0.129" app:layout_constraintBottom_toBottomOf="@+id/imageView"
                app:layout_constraintVertical_bias="0.017" android:text="-"/>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_marginStart="40dp"
                android:layout_marginEnd="40dp"
                android:layout_width="351dp"
                android:elevation="4dp"

                android:background="@drawable/white_background"
                android:layout_height="118dp"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" android:id="@+id/linearLayout"
                app:layout_constraintVertical_bias="0.842" app:layout_constraintTop_toTopOf="@+id/imageView"
                app:layout_constraintHorizontal_bias="0.65">
            <LinearLayout
                    android:orientation="vertical"
                    android:background="@drawable/top_banner_background"
                    android:layout_width="match_parent"
                    android:layout_margin="10dp"
                    android:layout_height="wrap_content" android:layout_weight="1">
                <ImageButton
                        android:layout_width="match_parent"
                        android:background="@drawable/white_background"
                        android:layout_height="62dp" app:srcCompat="@drawable/video_call"
                        android:id="@+id/profileButton" android:onClick="OpenProfilePage"/>
                <TextView
                        android:text="PROFILE"
                        android:textAlignment="center"
                        android:layout_marginBottom="8dp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/profileText"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/linearLayout2">
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="148dp"
            app:layout_constraintTop_toTopOf="@+id/constraintLayout2" android:layout_marginTop="288dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout2" app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintVertical_bias="0.09" android:id="@+id/linearLayout3">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="138dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:layout_height="120dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/white_background"
                    android:layout_height="wrap_content">

                <ImageButton
                        android:layout_width="112dp"
                        android:layout_height="117dp" app:srcCompat="@drawable/ic_8"
                        android:id="@+id/AboutUsImageButton"
                        android:background="@drawable/white_background"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.41"
                        app:layout_constraintVertical_bias="0.0" android:onClick="OpenAboutUs"/>
                <TextView
                        android:text="ABOUT US"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textSize="15sp"

                        android:layout_width="72dp"
                        android:layout_height="19dp" android:id="@+id/AboutUs"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.125" app:layout_constraintVertical_bias="0.0"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="134dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:layout_height="115dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/white_background"
                    android:layout_height="wrap_content">

                <ImageButton
                        android:layout_width="116dp"
                        android:layout_height="117dp" app:srcCompat="@drawable/ic_1"
                        android:id="@+id/PrivacyImageButton"
                        android:background="@drawable/white_background"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        android:onClick="OpenPolicy"/>
                <TextView
                        android:text="POLICY"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textSize="15sp"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/Policy"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.075" app:layout_constraintVertical_bias="0.042"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/white_background"
                    android:layout_height="wrap_content">

                <ImageButton
                        android:layout_width="109dp"
                        android:layout_height="113dp" app:srcCompat="@drawable/ic_6"
                        android:id="@+id/FAQImageButton"
                        android:background="@drawable/white_background"
                        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintVertical_bias="0.0" android:onClick="OpenFAQ"/>
                <TextView
                        android:text="FAQ"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textSize="15sp"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/mobileAnswer"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintHorizontal_bias="0.073" app:layout_constraintVertical_bias="0.074"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="131dp"
            app:layout_constraintTop_toTopOf="@+id/linearLayout3" android:layout_marginTop="148dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout2" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="157dp"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:layout_height="120dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/white_background"
                    android:layout_height="match_parent">

                <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" app:srcCompat="@drawable/btn_arraw1"
                        android:id="@+id/LogOutImageButton"
                        android:background="@drawable/white_background"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintVertical_bias="0.509" android:onClick="LogOut"/>
                <TextView
                        android:text="LOG OUT"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textSize="15sp"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/LogOut"
                        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.06" android:onClick="OpenLogOut"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>